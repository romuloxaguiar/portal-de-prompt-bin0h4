# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# OAuth Authentication Configuration
# Google OAuth
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# GitHub OAuth
VITE_GITHUB_CLIENT_ID=your_github_client_id_here
VITE_GITHUB_CALLBACK_URL=http://localhost:3000/auth/github/callback

# Microsoft OAuth
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
VITE_MICROSOFT_CALLBACK_URL=http://localhost:3000/auth/microsoft/callback

# Analytics Configuration
VITE_GA4_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token_here
VITE_ENABLE_ANALYTICS=true

# Feature Flags
VITE_ENABLE_REAL_TIME_COLLABORATION=true
VITE_ENABLE_PROMPT_OPTIMIZATION=true
VITE_ENABLE_VERSION_HISTORY=true
VITE_ENABLE_TEAM_WORKSPACES=true
VITE_ENABLE_AI_INTEGRATION=true
VITE_ENABLE_CUSTOM_TEMPLATES=true

# Performance Configuration
VITE_TARGET_RESPONSE_TIME=2000
VITE_MAX_CONCURRENT_REQUESTS=50
VITE_CACHE_TIMEOUT=3600

# Security Configuration
VITE_ALLOWED_ORIGINS=http://localhost:3000
VITE_MAX_TOKEN_AGE=3600
VITE_REQUIRE_MFA=true

# Region Configuration
VITE_APP_REGION=us-east-1

# Development Tools
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_API_LOGGING=true

# Rate Limiting
VITE_RATE_LIMIT_MAX_REQUESTS=1000
VITE_RATE_LIMIT_WINDOW_MS=3600000

# Web Vitals Reporting
VITE_ENABLE_WEB_VITALS=true
VITE_WEB_VITALS_SAMPLE_RATE=100

# Error Tracking
VITE_ERROR_TRACKING_ENABLED=true
VITE_ERROR_TRACKING_SAMPLE_RATE=100

# User Journey Tracking
VITE_USER_JOURNEY_TRACKING_ENABLED=true
VITE_USER_JOURNEY_SESSION_TIMEOUT=30